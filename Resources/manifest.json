{
    "manifest_version": 2,
    "name": "OpenInna",
    "version": "1.0",
    "description": "Open videos in IINA player",
    "default_locale": "zh_CN",
    "permissions": [
        "activeTab",
        "tabs",
        "contextMenus"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
} 